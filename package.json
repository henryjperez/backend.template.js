{
	"name": "backend-template",
	"version": "1.0.0",
	"description": "This is a template for start working with a fully configured backend repository of node.js",
	"main": "dist/index.js",
	"author": "Henry J. Perez <henryjosiasperezrangel@gmail.com>",
	"license": "MIT",
	"private": true,
	"scripts": {
		"start": "NODE_ENV=PRODUCTION node build/index.js",
		"dev": "NODE_ENV=DEVELOPMENT nodemon src/index.ts --config ./nodemon.json",
		"dev:docker": "docker compose --profile app_dev up",
		"db": "docker compose up -d",
		"build": "tsc --project tsconfig.json && tsc-alias && node copy_files.js",
		"lint": "eslint",
		"e2e": "jest --config ./jest.e2e.config.js --verbose",
		"e2e:coverage": "jest --config ./jest.e2e.config.js --verbose --coverage",
		"generate:codegen": "graphql-codegen",
		"generate:prisma": "prisma generate"
	},
	"dependencies": {
		"@apollo/server": "4.7.1",
		"@prisma/client": "4.15.0",
		"bcrypt": "5.1.0",
		"cors": "2.8.5",
		"dotenv": "16.1.3",
		"express": "4.18.2",
		"graphql": "16.6.0",
		"graphql-passport": "0.6.5",
		"joi": "17.9.2",
		"jsonwebtoken": "9.0.0",
		"morgan": "1.10.0",
		"multer": "1.4.5-lts.1",
		"passport": "0.6.0",
		"passport-jwt": "4.0.1",
		"passport-local": "1.0.0",
		"sharp": "0.32.1",
		"winston": "3.9.0"
	},
	"devDependencies": {
		"@babel/core": "7.22.1",
		"@babel/preset-env": "7.22.4",
		"@babel/preset-typescript": "7.21.5",
		"@graphql-codegen/cli": "4.0.1",
		"@graphql-codegen/typescript": "4.0.0",
		"@graphql-codegen/typescript-resolvers": "4.0.0",
		"@types/bcrypt": "5.0.0",
		"@types/express": "4.17.17",
		"@types/jest": "29.5.2",
		"@types/jsonwebtoken": "9.0.2",
		"@types/multer": "1.4.7",
		"@types/node": "20.2.5",
		"@types/passport-jwt": "3.0.8",
		"@types/passport-local": "1.0.35",
		"@types/supertest": "2.0.12",
		"babel-jest": "29.5.0",
		"eslint": "8.41.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-prettier": "4.2.1",
		"jest": "29.5.0",
		"nodemon": "2.0.22",
		"prettier": "2.8.8",
		"prisma": "4.15.0",
		"supertest": "6.3.3",
		"ts-jest": "29.1.0",
		"ts-node": "10.9.1",
		"tsc-alias": "1.8.6",
		"tsconfig-paths": "4.2.0",
		"typescript": "5.1.3"
	}
}
