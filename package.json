{
	"name": "backend-template",
	"version": "1.5.0",
	"description": "This is a template for start working with a fully configured backend repository of node.js",
	"main": "dist/index.js",
	"author": "Henry J. Perez <henryjosiasperezrangel@gmail.com>",
	"license": "MIT",
	"private": true,
	"scripts": {
		"start": "NODE_ENV=PRODUCTION node build/index.js",
		"dev": "NODE_ENV=DEVELOPMENT nodemon src/index.ts --config ./nodemon.json",
		"dev:docker": "docker compose --profile app_dev up",
		"db": "docker compose up -d",
		"db:migrate": "prisma migrate",
		"db:create-tables": "prisma db push",
		"build": "tsc --project tsconfig.json && tsc-alias && node copy_files.js",
		"lint": "eslint",
		"e2e": "jest --config ./jest.e2e.config.js --verbose",
		"e2e:coverage": "jest --config ./jest.e2e.config.js --verbose --coverage",
		"generate:codegen": "graphql-codegen",
		"generate:prisma": "prisma generate"
	},
	"dependencies": {
		"@apollo/server": "4.9.4",
		"@prisma/client": "5.4.1",
		"bcrypt": "5.1.1",
		"cors": "2.8.5",
		"dotenv": "16.3.1",
		"express": "4.18.2",
		"graphql": "16.8.1",
		"graphql-passport": "0.6.7",
		"joi": "17.11.0",
		"jsonwebtoken": "9.0.2",
		"morgan": "1.10.0",
		"multer": "1.4.5-lts.1",
		"passport": "0.6.0",
		"passport-jwt": "4.0.1",
		"passport-local": "1.0.0",
		"sharp": "0.32.6",
		"winston": "3.11.0"
	},
	"devDependencies": {
		"@babel/core": "7.23.0",
		"@babel/preset-env": "7.22.20",
		"@babel/preset-typescript": "7.23.0",
		"@graphql-codegen/cli": "5.0.0",
		"@graphql-codegen/typescript": "4.0.1",
		"@graphql-codegen/typescript-resolvers": "4.0.1",
		"@types/bcrypt": "5.0.0",
		"@types/express": "4.17.18",
		"@types/jest": "29.5.5",
		"@types/jsonwebtoken": "9.0.3",
		"@types/multer": "1.4.8",
		"@types/node": "20.8.3",
		"@types/passport-jwt": "3.0.10",
		"@types/passport-local": "1.0.36",
		"@types/supertest": "2.0.14",
		"babel-jest": "29.7.0",
		"eslint": "8.51.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-prettier": "5.0.0",
		"jest": "29.7.0",
		"nodemon": "3.0.1",
		"prettier": "3.0.3",
		"prisma": "5.4.1",
		"supertest": "6.3.3",
		"ts-jest": "29.1.1",
		"ts-node": "10.9.1",
		"tsc-alias": "1.8.8",
		"tsconfig-paths": "4.2.0",
		"typescript": "5.2.2"
	}
}
